  <client>
   <endpoint address="http://localhost:3902/Services/SubjectService.asmx" binding="basicHttpBinding" bindingConfiguration="SubjectServiceSoap" contract="Bi.Subject.SubjectServiceSoap" name="SubjectServiceSoap" />
