    <binding name="SubjectServiceSoap" />
    <binding name="StudentServiceSoap" />
    <binding name="ManagerServiceSoap" />
    <binding name="StudentResultServiceSoap" />
    <binding name="StudentResponseServiceSoap" />
    <binding name="ExamServiceSoap" />
